<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.black.ouc.domains">
	<class name="User" table="users">
		<id name="id" column="id">
			<generator class="identity"></generator>
		</id>
		<property name="account" column="account"></property>
		<property name="password" column="password"></property>
		<property name="mail" column="mail"></property>
		<property name="roomnum" column="roomnum"></property>
	</class>
	<class name="Room" table="rooms">
		<id name="id" column="id">
			<generator class="identity"></generator>
		</id>
		<property name="house" column="house"></property>
		<property name="room" column="room"></property>
		<property name="area" column="area"></property>
		<property name="ect" column="ect"></property>
	</class>
</hibernate-mapping> 